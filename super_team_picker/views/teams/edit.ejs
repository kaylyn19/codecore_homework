<%- include("../../partials/header")%>
<form action="/cohorts/<%= record.id %>" method=POST>
  <div class="form-group">
    <label for="exampleFormControlInput1">Logo Url</label>
    <input type="url" name='url' class="form-control" id="exampleFormControlInput1" placeholder="https://example.com/some.png" value="<%= record.logoUrl%>">
  </div>

  <div class="form-group">
    <label for="exampleFormControlInput1">Name</label>
    <input type="text" name='name' class="form-control" id="exampleFormControlInput1" value="<%= record.name%>">
  </div>

    <label for="exampleFormControlInput1">Members</label>
    <div class="input-group">
        <textarea class="form-control" name="members" aria-label="With textarea"><%= record.members%></textarea>
    </div>
    <input type="hidden" name="_method" value='PUT'>
    <button type="submit" value="Update" class="btn btn-primary">Update</button>
</form>
<%- include("../../partials/footer")%>